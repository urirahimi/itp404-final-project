{{#if model}}
<table style="width:50%" id="todosTable">
    <h1>Items on Todo List</h1>
    <tr>
        <th>Complete</th>
        <th>Title</th>
        <th>Due Date</th>
    </tr>
    {{#each model as |todo|}} {{#if todo.starred}}
    {{else}}
    <tr>
        <td>
            <StarButton @starred={{todo.starred}} @onClick={{action "toggleStarred" todo}} />
        </td>
        <td data-test="emails">
            {{#link-to "todo" todo.id}} {{todo.subject}} {{/link-to}}
        </td>
        <td>
            {{todo.dueDate}}
        </td>
    </tr>
    {{/if}}
    {{/each}}
</table>
{{else}}

<EmptyTodo @completed={{false}} />
{{/if}}
{{outlet}}